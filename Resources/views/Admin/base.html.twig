{% extends '@VlabsCms/base.html.twig' %}

{% block stylesheets %}

    {{ parent() }}

    <link href="{{ asset('bundles/vlabscms/vendor/font-awesome/css/font-awesome.min.css') }}" type="text/css" rel="stylesheet"/>
    <link href="{{ asset('bundles/vlabscms/vendor/summernote/summernote.min.css') }}" type="text/css" rel="stylesheet"/>
    <link href="{{ asset('bundles/vlabscms/vendor/select2/css/select2.min.css') }}" type="text/css" rel="stylesheet"/>

    <link href="{{ asset('bundles/vlabscms/vendor/jquery-simplecolorpicker/jquery.simplecolorpicker.css') }}" type="text/css" rel="stylesheet"/>
    <link href="{{ asset('bundles/vlabscms/vendor/sweet-alert/css/sweet-alert.css') }}" type="text/css" rel="stylesheet"/>
    <link href="{{ asset('bundles/vlabscms/vendor/jquery-fileupload/css/jquery.fileupload.css') }}" type="text/css" rel="stylesheet"/>

    <link href="{{ asset('bundles/vlabscms/css/admin.css') }}" type="text/css" rel="stylesheet"/>

{% endblock %}

{% block flash %}
    {{ include('@VlabsCms/Admin/flash.html.twig') }}
{% endblock %}

{% block javascripts %}

    {{ parent() }}

    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/jquery/jquery.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/jquery-ui/jquery-ui.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/select2/js/select2.full.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/bootstrap-confirmation/bootstrap-confirmation.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/summernote/summernote.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/bootbox/bootbox.min.js') }}" type="text/javascript"></script>

    <script src="{{ asset('bundles/vlabscms/vendor/jquery-simplecolorpicker/jquery.simplecolorpicker.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/jquery-fileupload/js/jquery.fileupload.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/jquery-fileupload/js/jquery.fileupload-process.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/jquery-fileupload/js/jquery.fileupload-validate.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/vendor/sweet-alert/js/sweet-alert.min.js') }}" type="text/javascript"></script>

    {% if app.request.locale == 'fr' %}
        <script src="{{ asset('bundles/vlabscms/vendor/select2/js/i18n/fr.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/vlabscms/vendor/summernote/lang/summernote-fr.min.js') }}" type="text/javascript"></script>
    {% endif %}

    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-url.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-link.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-mailto.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-pdf.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-picture.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-style.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-template.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/summernote/vlabs-video.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/vlabscms/js/admin.js') }}" type="text/javascript"></script>

{% endblock %}